{
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "keyword-spacing": [
        "error",
        {}
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "no-console": "off",
      "no-var": [
        "error"
      ],
      "prefer-arrow-callback": [
        "error",
        {}
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  }
  